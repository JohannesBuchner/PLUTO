# Grid size
# n_cell = 32 32 32
n_cell = 64 64 64
# n_cell = 128 128 128
# n_cell = 256 256 256

# Maximum box size
maxboxsize = 16

# Domain domain - physical coordinates
# prob_lo is the origin of the coordinate system
prob_lo = -2.0 -2.0 -2.0 
# prob_hi is the high point of the domain in the x direction
# Note:  dx = dy = dz
prob_hi = 2.0

# insideRegular != 0 -> Domain is on the inside  of the polynomial
# insideRegular == 0 -> Domain is on the outside of the polynomial
insideRegular = 0

# Order of the polynomial
power = 5

